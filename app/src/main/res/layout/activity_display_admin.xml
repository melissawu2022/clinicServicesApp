<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    tools:context=".AdminFeatures.AdminControlPanel">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="106dp"
        android:layout_height="30dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="226dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="475dp"
        android:text="Welcome,"
        android:textColor="#090000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/inputUserName2"
        android:layout_width="144dp"
        android:layout_height="33dp"
        android:layout_marginStart="133dp"
        android:layout_marginTop="256dp"
        android:layout_marginEnd="134dp"
        android:layout_marginBottom="442dp"
        android:gravity="center"
        android:text="@string/emptyStringName"
        android:textAlignment="center"
        android:textColor="#090303"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="229dp"
        android:layout_height="40dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="346dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="346dp"
        android:gravity="center"
        android:text="You are logged in as:"
        android:textColor="#0E0101"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/role2"
        android:layout_width="200dp"
        android:layout_height="39dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="385dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="307dp"
        android:gravity="center"
        android:text="@string/emptyStringRole"
        android:textColor="#0E0101"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom|center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <Button
            android:id="@+id/ManageAcc"
            android:background="@drawable/custom_button"
            android:textColor="@color/Pure_White"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Manage Account" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="5dp" />

        <Button
            android:id="@+id/ManagerServ"
            android:background="@drawable/custom_button"
            android:textColor="@color/Pure_White"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Manage Service" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>